{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "downlevelIteration": true
  },
  "extends": "expo/tsconfig.base"
}
